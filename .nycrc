{
	"cache": false,
	"check-coverage": false,
	"extension": [
		".ts"
	],
	"include": [
		"src/**/*.ts"
	],
	"exclude": [
		"coverage/**",
		"node_modules/**",
		"**/*.d.ts",
		"**/*.test.ts",
		"tests/*",
		".eslintrc.js",
		"webpack.config.js"
	],
	"sourceMap": true,
	"reporter": [
		"html",
		"text",
		"text-summary"
	],
	"all": true,
	"instrument": true
}